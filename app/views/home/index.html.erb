<h1>Hello, Rails!</h1>
<%= link_to "My Blog", posts_path %>

<br />
<br />

<%= form_tag("/", :method => "get") do %>
	<%= label_tag :symbol %>
	<%= text_field_tag :symbol %>

	<%= submit_tag "Get Data" %>
	
	<br />
	<br />


<% if params["symbol"] != nil %>

<%

$temp = Array.new

%>
	
<%

YahooFinance::get_historical_quotes_days(params["symbol"],2800) do |row|
	$temp.push Float(row[6])
%>

<% end %>


<br />
<%= image_tag(Gchart.line(:data => $temp.reverse)) %>
<br />


<% end %>
<% end %>



